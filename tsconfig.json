{
	"compileOnSave": false,
	"compilerOptions": {
		"allowSyntheticDefaultImports": false,
		"target": "es5",
		"module": "es2015",
		"jsx": "preserve",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"removeComments": true,
		"strictNullChecks": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"lib": ["es2015", "es2016", "dom"]
	}
}
